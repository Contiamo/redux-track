  "license": "MIT",
  "scripts": {
    "test": "jest --coverage",
    "precommit": "npm run test",
    "prepublish": "yarn tsc",
    "ci": "npm run test --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "files": ["dist/index.js"],
  "dependencies": {
    "redux": "^4.0.1"
  },
  "peerDependencies": {
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.4",
    "typescript": "next"
  },
  "jest": {
    "transform": {
      "\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleFileExtensions": ["ts", "js"],
    "testRegex": ".*\\.test\\.ts$",
    "mapCoverage": true
  }
}
